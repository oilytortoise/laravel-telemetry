{
    "name": "oilytortoise/laravel-telemetry",
    "description": "A simple package for integrating a Laravel application with BetterStack's Telemetry service.",
    "type": "library",
    "minimum-stability": "beta",
    "require": {
        "logtail/monolog-logtail": "^3.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "php": "^8.3.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oilytortoise\\LaravelTelemetry\\": "src/"
        }
    },
    "authors": [
        {
            "name": "oilytortoise",
            "email": "dev@oilytortoise.xyz"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Oilytortoise\\LaravelTelemetry\\TelemetryServiceProvider"
            ]
        }
    }
}
